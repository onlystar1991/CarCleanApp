<form action="/cars/create" enctype="multipart/form-data" method="post">

    <div class="field">
		<label>Token:</label>
		<input type="text" name="auth_token">
	</div>
    <div class="field">
        <label>Car name:</label>
        <input type="text" name="car_name">
    </div>
    <div class="field">
		<label>File:</label>
		<input type="file" name="car_image"> 
	</div>
    
    <div class="actions">
        <input type="submit" name="submit" value="save">
    </div>
</form>
